vec4 onePixelRadius(sampler image, const vec2 xy, float width)
{
    vec4 pix;
    
    float radius = 1.0;
    
    float redSum = 0.0;
    float greenSum = 0.0;
    float blueSum = 0.0;
    
    float newX = 0.0;
    
    for(float f=-radius; f<=radius; f++){
    
        newX = ((xy.x+f) < 0.0) ? xy.x : xy.x+f;
        newX = ((xy.x+f) >= width) ? width - 1.0 : newX;
    
        vec4 tempPix = sample(image, samplerTransform(image, vec2(newX, xy.y)));
        
        redSum += tempPix.r;
        greenSum += tempPix.g;
        blueSum += tempPix.b;
    }
    
    pix.r = redSum/(radius*2.0 + 1.0);
    pix.g = greenSum/(radius*2.0 + 1.0);
    pix.b = blueSum/(radius*2.0 + 1.0);
    
    return pix;
}

