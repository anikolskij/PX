kernel vec4 HorizontalPassKernel(sampler image, sampler maskImage, float blurRadius, float imageWidth)
{
    vec4 pix;

    pix = unpremultiply(sample(image, samplerCoord(image)));    
   
    vec4 maskPix;
    maskPix = unpremultiply(sample(maskImage, samplerCoord(maskImage)));
    
    const vec2 xy = destCoord();
    
    vec4 newPix;
    
    float newRadius = blurRadius*maskPix.r;
    int intRadius = int(newRadius);
    float radius = float(intRadius);    
    
   
    newPix = (radius == 0.0) ? pix : (radius == 1.0) ? onePixelRadius(image, xy, imageWidth) : (radius == 2.0) ? twoPixelsRadius(image, xy, imageWidth) : (radius == 3.0) ? threePixelsRadius(image, xy, imageWidth) : (radius == 4.0) ? fourPixelsRadius(image, xy, imageWidth) : (radius == 5.0) ? fivePixelsRadius(image, xy, imageWidth) : (radius == 6.0) ? sixPixelsRadius(image, xy, imageWidth) : (radius == 7.0) ? sevenPixelsRadius(image, xy, imageWidth) : (radius == 8.0) ? eightPixelsRadius(image, xy, imageWidth) : (radius == 9.0) ? ninePixelsRadius(image, xy, imageWidth) : tenPixelsRadius(image, xy, imageWidth);

    
    pix.rgb = newPix.rgb;
    
    return premultiply(pix); 
}